{
  "name": "chronos-tools",
  "version": "1.0.0",
  "description": "Chronos Client Tools",
  "main": "dist/index.js",
  "scripts": {
  "test": "mocha --compilers js:babel-core/register src/index.test.js",
  "prebuild": "rimraf dist",
  "build": "npm-run-all --parallel build:*",
  "build:main": "babel --copy-files --out-dir dist src",
  "build:umd": "webpack --output-filename chronos.sdk.js",
  "build:umd.min": "webpack --output-filename chronos.sdk.min.js -p"
  },
  "babel": {
   "presets": [
     "es2015",
     "stage-2"
   ]
 },
  "repository": {
  "type": "git",
  "url": "git@github.com:iris-platform/chronos-tools.git"
  },
  "keywords": [
  "iris"
  ],
  "dependencies": {
    "request": "2.81.0",
    "cli-table":  "0.3.1",
    "dockerode": "2.5.0"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-2": "6.11.0",
    "chai": "^3.5.0",
    "json-loader": "0.5.4",
    "mocha": "^2.5.3",
    "mocha-junit-reporter": "^1.11.1",
    "npm-run-all": "2.3.0",
    "rimraf": "2.5.3",
    "webpack": "1.13.1"
  }
}
